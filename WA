/*
#include <stdio.h>
*/

int GCD3(int a,int b,int c)
{
    int n;
    for(int i=0;i<(a+b+c)/3;i++)
    {
        if(a%i==0 && b%i==0 && c%i==0)
        {
            return i;
        }
        else
        {
            return i;
        }
    }
}

